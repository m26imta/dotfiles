# export
export EDITOR=vim
export VISUAL=vim
export PATH=$HOME/bin:$PATH

# source
# cargo
[[ -f "$HOME/.cargo/env" ]] && . "$HOME/.cargo/env"
[[ -f "$HOME/.myalias" ]] && . "$HOME/.myalias"

alias cls=clear
alias gitlog='git log --graph --all --oneline'
alias v=nvim

alias l='ls -CF'
alias ll='ls -lahF'
alias la='ls -A'
alias e='explorer.exe .'


